<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<modded_inputs>
    <inputs>
        <actions>
            <input name="Copy" loc="Copy"/>
            <input name="Cut" loc="Cut"/>
            <input name="Delete" loc="Delete"/>
            <input name="Lock" loc="Lock"/>
            <input name="New" loc="New File"/>
            <input name="Open" loc="Open File"/>
            <input name="Paste" loc="Paste"/>
            <input name="Redo" loc="Redo"/>
            <input name="SaveAs" loc="SaveAs"/>
            <input name="Save" loc="Save File"/>
            <input name="Undo" loc="Undo"/>
            <input name="Unlock" loc="Unlock"/>
            <input name="CursorToggle" loc="Toggle Cursor"/>
        </actions>
        
        <sorting name="Editor" loc="EDITOR">
            <input name="Copy"/>
            <input name="Cut"/>
            <input name="Delete"/>
            <input name="Lock"/>
            <input name="New"/>
            <input name="Open"/>
            <input name="Paste"/>
            <input name="Redo"/>
            <input name="SaveAs"/>
            <input name="Save"/>
            <input name="Undo"/>
            <input name="Unlock"/>
            <input name="CursorToggle"/>
        </sorting>
    </inputs>

    <preset>
        <input name="Copy">
            <btn name="kLControl">
                <btn name="kC"/>
            </btn>
        </input>
        <input name="Cut">
            <btn name="kLControl">
                <btn name="kX"/>
            </btn>
        </input>
        <input name="Delete">
            <btn name="kDelete"/>
        </input>
        <input name="Lock">
            <btn name="kLControl">
                <btn name="kL"/>
            </btn>
        </input>
        <input name="New">
            <btn name="kLControl">
                <btn name="kN"/>
            </btn>
        </input>
        <input name="Open">
            <btn name="kLControl">
                <btn name="kO"/>
            </btn>
        </input>
        <input name="Paste">
            <btn name="kLControl">
                <btn name="kV"/>
            </btn>
        </input>
        <input name="Redo">
            <btn name="kLControl">
                <btn name="kY"/>
            </btn>
        </input>
        <input name="SaveAs">
            <btn name="kLControl" >
                <event name="hold" />
            </btn>
            <btn name="kLShift">
                <btn name="kS"/>
            </btn>
        </input>
        <input name="Save">
            <btn name="kLControl">
                <btn name="kS"/>
            </btn>
        </input>
        <input name="Undo">
            <btn name="kLControl">
                <btn name="kZ"/>
            </btn>
        </input>
        <input name="Unlock">
            <btn name="kLControl">
                <btn name="kL"/>
            </btn>
        </input>
        <input name="CursorToggle">
            <btn name="kSpace"/>
        </input>
    </preset>
</modded_inputs>